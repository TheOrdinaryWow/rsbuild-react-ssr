{
  "name": "rsbuild-react-ssr",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "node server/development.js",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "preview": "bun run server/production.ts",
    "build:compile": "rsbuild build && bun build server/production.ts --target bun --production --minify --server-components --outfile dist/compile/main.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@rsbuild/core": "^1.3",
    "@rsbuild/plugin-react": "^1.3",
    "@types/bun": "latest",
    "@types/express": "^5",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1",
    "@types/react-dom": "^19.1",
    "express": "^5",
    "rimraf": "latest",
    "typescript": "^5.8.3"
  }
}
